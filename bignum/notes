m = 0xE92E40AD6F281C8A082AFDC49E1372659455BEC8CEEA043A614C835B7FE9EFF5
mu = (2^522)//m
x = 0xE92E40AD6F281C8A082AFDC49E1372659455BEC8CEEA043A614C835B7FE9EFFF * 0xE92E40AD6F281C8A082AFDC49E1372659455BEC8CEEA043A614C835B7FE9EFFF
q1 = x//(2^232)
q2 = q1 * mu
q3 = q2//(2^290)
r1 = x%(2^290)
r2 = (q3*m)%(2^290)
base = 0x0fdf8e8d507451333a4f204a0835023f69a3e832e667b893137efaabc17f9db6
exp =  0x0f1086795eb766f9028c44236516fc40c844134f007b3dfc6722c20c6df710a4
result = pow(base, exp, m)
print(hex(result))

